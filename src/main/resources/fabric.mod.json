	"name": "SpaceCraft",
	"description": "A friendly mod for space lovers.",
	"authors": [
		"Kim Chu",
		"David Estrella"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
